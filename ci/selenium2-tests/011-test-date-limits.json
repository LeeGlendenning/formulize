{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://localhost/index.php"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "uname"
      },
      "text": "admin"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "pass"
      },
      "text": "password"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "input[type=\"submit\"]"
      }
    },
    {
      "type": "waitForTextPresent",
      "text": "General Forms"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Project"
      }
    },
    {
      "type": "waitForTextPresent",
      "text": "Entries in "
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "formulize_addButton"
      }
    },
    {
      "type": "waitForTextPresent",
      "text": "You can make a new entry by filling in the form below"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "css selector",
        "value": "td.formulize-input-project_start_date input"
      },
      "text": ""
    },
    {
      "type": "sendKeysToElement",
      "locator": {
        "type": "css selector",
        "value": "td.formulize-input-project_start_date input"
      },
      "text": "2005-06-07\t"
    },
    {
      "type": "pause",
      "waitTime": "1543"
    },
    {
      "type": "verifyAlertPresent"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}