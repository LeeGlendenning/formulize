{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "store",
      "text": "admin",
      "variable": "username"
    },
    {
      "type": "store",
      "text": "password",
      "variable": "password"
    },
    {
      "type": "get",
      "url": "http://localhost/index.php"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "uname"
      },
      "text": "${username}"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "pass"
      },
      "text": "${password}"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "input[type=\"submit\"]"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Admin"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Client"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Screens"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Test Template"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "View Screen"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "formulize_globalQuickSearch"
      },
      "text": "toronto"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Client Name"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Manitobans for Toronto"
    },
    {
      "type": "verifyTextPresent",
      "text": "Toronto"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "firstbox"
      },
      "text": "humans"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Client Name"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Humans For Humanity"
    },
    {
      "type": "verifyTextPresent",
      "negated": true,
      "text": "Manitobans for Toronto"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}